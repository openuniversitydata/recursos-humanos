    rdfs:label "estudio en" ;
    rdfs:domain univPDI:PersonalDocenteInvestigador ;
    rdfs:range univPDI:UniversidadDoctorado .

univPDI:formaParteDe a owl:ObjectProperty ;
    rdfs:label "forma parte de" ;
    rdfs:domain univPDI:PersonalDocenteInvestigador ;
    rdfs:range univPDI:AreaConocimiento .

univPDI:perteneceA a owl:ObjectProperty ;
    rdfs:label "pertenece a" ;
    rdfs:domain univPDI:PersonalDocenteInvestigador ;
    rdfs:range univPDI:UnidadResponsable .

univPDI:trabajaEn a owl:ObjectProperty ;
    rdfs:label "trabaja en" ;
