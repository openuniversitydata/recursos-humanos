    rdfs:label "Provincia Residencia" ;
    rdfs:subClassOf univPDI:ComunidadResidencia .

univPDI:UnidadResponsable a owl:Class ;
    rdfs:label "Unidad Responsable" .

univPDI:UniversidadDoctorado a owl:Class ;
    rdfs:label "Universidad Doctorado" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty univPDI:estaUbicadaEn ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty univPDI:estaUbicadaEn ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty univPDI:estaUbicadaEn ],
        org:Organization .

geosparql:Point a owl:Class ;
    rdfs:label "Point" ;
    rdfs:subClassOf geosparql:Geometry .

geosparql:SpatialObject a owl:Class ;
    rdfs:label "Spatial Object" .

univPDI:ContinenteDoctorado a owl:Class ;
    rdfs:label "Continente Doctorado" .

univPDI:ContinenteNacionalidad a owl:Class ;
    rdfs:label "Continente Nacionalidad" .

univPDI:Universidad a owl:Class ;
    rdfs:label "Universidad" ;
    rdfs:subClassOf org:Organization .

univPDI:estaUbicadaEn a owl:ObjectProperty ;
    rdfs:label "esta ubicada en" ;
    rdfs:domain univPDI:UniversidadDoctorado ;
    rdfs:range univPDI:AgregacionPaisesDoctorado,
        univPDI:ContinenteDoctorado,
        univPDI:PaisDoctorado .

geosparql:Geometry a owl:Class ;
    rdfs:label "Geometry" ;
    rdfs:subClassOf geosparql:SpatialObject .

org:Organization a owl:Class ;
    rdfs:label "Organization" .

geosparql:Feature a owl:Class ;
    rdfs:label "Feature" ;
    rdfs:subClassOf geosparql:SpatialObject .

schema:Person a owl:Class ;
    rdfs:label "Person" .

univPDI:posee a owl:ObjectProperty ;
    rdfs:label "posee" ;
    rdfs:domain univPDI:PersonalDocenteInvestigador ;
    rdfs:range univPDI:ContinenteNacionalidad,
        univPDI:MunicipioResidencia,
        univPDI:PaisNacionalidad,
        univPDI:ProvinciaResidencia .

univPDI:PersonalDocenteInvestigador a owl:Class ;
    rdfs:label "Personal Docente Investigador" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty univPDI:estudioEn ],
        [ a owl:Restriction ;
            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty univPDI:trabajaEn ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty univPDI:perteneceA ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty univPDI:posee ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty univPDI:posee ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty univPDI:posee ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty univPDI:posee ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty univPDI:posee ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty univPDI:posee ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty univPDI:formaParteDe ],
        schema:Person .

