    rdfs:label "Agregacion Paises Doctorado" ;
    rdfs:subClassOf univPDI:ContinenteDoctorado .

univPDI:Anio a owl:Class ;
    rdfs:label "Anio" .

univPDI:AreaConocimiento a owl:Class ;
    rdfs:label "Area Conocimiento" .

univPDI:ComunidadResidencia a owl:Class ;
    rdfs:label "Comunidad Residencia" .

univPDI:MunicipioResidencia a owl:Class ;
    rdfs:label "Municipio Residencia" ;
    rdfs:subClassOf univPDI:ProvinciaResidencia,
        geosparql:Feature .

univPDI:PaisDoctorado a owl:Class ;
    rdfs:label "Pais Doctorado" ;
    rdfs:subClassOf univPDI:ContinenteDoctorado,
        geosparql:Feature .

