 @prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix geo: <http://www.owl-ontologies.com/geo#> .
@prefix geosparql: <http://www.owl-ontologies.com/geosparql#> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix univPAS: <http://vocab.universidadesabiertas.es/ontology/recursos-humanos#PAS> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

dct:identifier a owl:DatatypeProperty ;
    rdfs:label "identifier" ;
    rdfs:domain univPAS:Universidad ;
    rdfs:range xsd:string .

schema:birthDate a owl:DatatypeProperty ;
    rdfs:label "birth date" ;
    rdfs:domain schema:Person .

schema:familyName a owl:DatatypeProperty ;
    rdfs:label "family name" ;
    rdfs:domain schema:Person .

schema:gender a owl:DatatypeProperty ;
    rdfs:label "gender" ;
    rdfs:domain schema:Person .

schema:givenName a owl:DatatypeProperty ;
    rdfs:label "given name" ;
    rdfs:domain schema:Person .

schema:identifier a owl:DatatypeProperty ;
    rdfs:label "identifier" ;
    rdfs:domain schema:Person .

univPAS: a owl:Ontology ;
    dc:creator "VÃ­ctor Morcuende" ;
    dc:description "Ontology code created by Chowlk" ;
    dc:title "PAS Ontology" ;
    owl:versionInfo "0.0.1" .

univPAS:AgregacionPaisesNacionalidad a owl:Class ;
    rdfs:label "Agregacion Paises Nacionalidad" ;
    rdfs:subClassOf univPAS:ContinenteNacionalidad .

univPAS:anioIncorporacion a owl:DatatypeProperty ;
    rdfs:label "anio incorporacion" ;
    rdfs:domain univPAS:PersonalAdministracionServicios ;
    rdfs:range xsd:integer .

univPAS:anioNacimiento a owl:DatatypeProperty ;
    rdfs:label "anio nacimiento" ;
    rdfs:domain univPAS:PersonalAdministracionServicios ;
    rdfs:range xsd:integer .

univPAS:cuerpoEscalaPAS a owl:ObjectProperty ;
    rdfs:label "cuerpo escala p a s" ;
    rdfs:domain univPAS:PersonalAdministracionServicios .

univPAS:dedicacion a owl:ObjectProperty ;
    rdfs:label "dedicacion" ;
    rdfs:domain univPAS:PersonalAdministracionServicios .

univPAS:estaAsociadoA a owl:ObjectProperty ;
    rdfs:label "esta asociado a" ;
    rdfs:domain univPAS:Anio ;
    rdfs:range univPAS:Universidad .

univPAS:genero a owl:ObjectProperty ;
    rdfs:label "genero" ;
    rdfs:domain univPAS:PersonalAdministracionServicios .

univPAS:indCargoRemunerado a owl:DatatypeProperty ;
    rdfs:label "ind cargo remunerado" ;
    rdfs:domain univPAS:PersonalAdministracionServicios ;
    rdfs:range xsd:boolean .

univPAS:indImparteDocencia a owl:DatatypeProperty ;
    rdfs:label "ind imparte docencia" ;
    rdfs:domain univPAS:PersonalAdministracionServicios ;
    rdfs:range xsd:boolean .

univPAS:indOtraUnidad a owl:DatatypeProperty ;
    rdfs:label "ind otra unidad" ;
    rdfs:domain univPAS:PersonalAdministracionServicios ;
    rdfs:range xsd:boolean .

univPAS:numHorasSemanalesTiempoParcial a owl:DatatypeProperty ;
    rdfs:label "num horas semanales tiempo parcial" ;
    rdfs:domain univPAS:PersonalAdministracionServicios ;
    rdfs:range xsd:integer .

univPAS:numTrienios a owl:DatatypeProperty ;
    rdfs:label "num trienios" ;
    rdfs:domain univPAS:PersonalAdministracionServicios ;
    rdfs:range xsd:integer .

univPAS:servicioPrestado a owl:ObjectProperty ;
    rdfs:label "servicio prestado" ;
    rdfs:domain univPAS:PersonalAdministracionServicios .

univPAS:situacionAdministrativa a owl:ObjectProperty ;
    rdfs:label "situacion administrativa" ;
    rdfs:domain univPAS:PersonalAdministracionServicios .

univPAS:tipoContrato a owl:ObjectProperty ;
    rdfs:label "tipo contrato" ;
    rdfs:domain univPAS:PersonalAdministracionServicios .

univPAS:tipoPersonal a owl:ObjectProperty ;
    rdfs:label "tipo personal" ;
    rdfs:domain univPAS:PersonalAdministracionServicios .

univPAS:titulacionExigida a owl:ObjectProperty ;
    rdfs:label "titulacion exigida" ;
    rdfs:domain univPAS:PersonalAdministracionServicios .

geo:lat a owl:DatatypeProperty ;
    rdfs:label "lat" ;
    rdfs:domain geosparql:Point ;
    rdfs:range xsd:double .

geo:long a owl:DatatypeProperty ;
    rdfs:label "long" ;
    rdfs:domain geosparql:Point ;
    rdfs:range xsd:double .

geosparql:asWKT a owl:DatatypeProperty ;
    rdfs:label "as w k t" ;
    rdfs:domain geosparql:Geometry ;
    rdfs:range xsd:wKTLiteral .

geosparql:hasGeometry a owl:ObjectProperty ;
    rdfs:label "has geometry" ;
    rdfs:domain geosparql:Geometry ;
    rdfs:range geosparql:Feature .

org:hasSite a owl:ObjectProperty ;
    rdfs:label "has site" ;
    rdfs:domain org:Organization ;
    rdfs:range org:Site .

univPAS:Anio a owl:Class ;
    rdfs:label "Anio" .

univPAS:ComunidadResidencia a owl:Class ;
    rdfs:label "Comunidad Residencia" .

univPAS:MunicipioResidencia a owl:Class ;
    rdfs:label "Municipio Residencia" ;
    rdfs:subClassOf univPAS:ProvinciaResidencia,
        geosparql:Feature .

univPAS:OtraUnidad a owl:Class ;
    rdfs:label "Otra Unidad" .

univPAS:PaisNacionalidad a owl:Class ;
    rdfs:label "Pais Nacionalidad" ;
    rdfs:subClassOf univPAS:ContinenteNacionalidad,
        geosparql:Feature .

univPAS:perteneceA a owl:ObjectProperty ;
    rdfs:label "pertenece a" ;
    rdfs:domain univPAS:PersonalAdministracionServicios ;
    rdfs:range univPAS:OtraUnidad .

univPAS:trabajaEn a owl:ObjectProperty ;
    rdfs:label "trabaja en" ;
    rdfs:domain univPAS:PersonalAdministracionServicios ;
    rdfs:range univPAS:Universidad .

org:Site a owl:Class ;
    rdfs:label "Site" ;
    rdfs:subClassOf geosparql:Feature .

univPAS:ProvinciaResidencia a owl:Class ;
    rdfs:label "Provincia Residencia" ;
    rdfs:subClassOf univPAS:ComunidadResidencia .

geosparql:Point a owl:Class ;
    rdfs:label "Point" ;
    rdfs:subClassOf geosparql:Geometry .

geosparql:SpatialObject a owl:Class ;
    rdfs:label "Spatial Object" .

org:Organization a owl:Class ;
    rdfs:label "Organization" .

univPAS:ContinenteNacionalidad a owl:Class ;
    rdfs:label "Continente Nacionalidad" .

univPAS:Universidad a owl:Class ;
    rdfs:label "Universidad" ;
    rdfs:subClassOf org:Organization .

geosparql:Geometry a owl:Class ;
    rdfs:label "Geometry" ;
    rdfs:subClassOf geosparql:SpatialObject .

geosparql:Feature a owl:Class ;
    rdfs:label "Feature" ;
    rdfs:subClassOf geosparql:SpatialObject .

schema:Person a owl:Class ;
    rdfs:label "Person" .

univPAS:posee a owl:ObjectProperty ;
    rdfs:label "posee" ;
    rdfs:domain univPAS:PersonalAdministracionServicios ;
    rdfs:range univPAS:ContinenteNacionalidad,
        univPAS:MunicipioResidencia,
        univPAS:PaisNacionalidad,
        univPAS:ProvinciaResidencia .

univPAS:PersonalAdministracionServicios a owl:Class ;
    rdfs:label "Personal Administracion Servicios" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty univPAS:perteneceA ],
        [ a owl:Restriction ;
            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty univPAS:trabajaEn ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty univPAS:posee ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty univPAS:posee ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty univPAS:posee ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty univPAS:posee ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty univPAS:posee ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty univPAS:posee ],
        schema:Person .

